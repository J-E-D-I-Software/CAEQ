{
    "name": "caeq-backend",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "update-env": "node ./utils/updateEnv.js",
        "test": "cross-env NODE_ENV=dev && jest --forceExit --runInBand",
        "start": "cross-env NODE_ENV=production node index.js",
        "start:test": "cross-env NODE_ENV=testing nodemon index.js",
        "start:dev": "cross-env NODE_ENV=development nodemon index.js",
        "start:prod": "cross-env NODE_ENV=production nodemon index.js",
        "deploy-prod": "npm run update-env production && firebase deploy --only functions:prod",
        "deploy-test": "npm run update-env testing && firebase deploy --only functions:test"
    },
    "dependencies": {
        "@google-cloud/storage": "^5.8.5",
        "@sendgrid/mail": "^7.7.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "busboy": "^0.2.14",
        "compression": "^1.7.4",
        "content-type": "^1.0.4",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "debug": "~2.6.9",
        "dotenv": "^16.3.1",
        "express": "~4.16.1",
        "express-fileupload": "^1.2.1",
        "express-mongo-sanitize": "^2.2.0",
        "express-rate-limit": "^7.0.1",
        "firebase": "^8.6.8",
        "firebase-admin": "^11.5.0",
        "firebase-functions": "^4.2.0",
        "helmet": "^7.0.0",
        "html-to-text": "^9.0.5",
        "http-errors": "~1.6.3",
        "jade": "~1.11.0",
        "jest": "^29.5.0",
        "jsonwebtoken": "^9.0.2",
        "mongodb": "^6.0.0",
        "mongodb-memory-server": "^8.12.2",
        "mongoose": "^5.13.20",
        "morgan": "~1.9.1",
        "multer": "^1.4.2",
        "nodemailer": "^6.9.5",
        "nodemon": "^3.0.1",
        "pug": "^3.0.2",
        "raw-body": "^2.3.2",
        "sharp": "^0.32.5",
        "supertest": "^6.3.3",
        "utils": "^0.3.1",
        "validator": "^13.11.0",
        "xhr2": "^0.2.1"
    },
    "jest": {
        "testEnvironment": "node",
        "testTimeout": 60000,
        "coveragePathIgnorePatterns": [
            "/node_modules/"
        ]
    }
}
